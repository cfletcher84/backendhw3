# # Problem 1, task 1

grades = [85, 90, 78, 88, 76, 95, 89, 80, 72, 93]
grades.sort(reverse=True)
print(grades)

# Task 2

list_length = len(grades)
list_sum = sum(grades)
list_average = list_sum/list_length 
print(list_average)

# Task 3

grades[6] = "Fail"
grades[7] = "Fail"
grades[8] = "Fail"
grades[9] = "Fail"
print(grades)


# Question 2 Task 1

submitted = ["Alice", "Bob", "Charlie", "David"]
attended = ["Charlie", "Eve", "Alice", "Frank"]
did_both = []
did_both_list = []
full_list = submitted + attended

for i in full_list:
    if i not in did_both:
        did_both.append(i)
    else:
        did_both_list.append(i)
    
print("These students attended and submitted: " + str(did_both_list))

#  Task 2

print(submitted == attended)


# Task 3

submitted = ["Alice", "Bob", "Charlie", "David"]
attended = ["Charlie", "Eve", "Alice", "Frank"]
full_list = attended + submitted
print(full_list)
full_list.remove("Charlie")
full_list.remove("Charlie")
full_list.remove("Alice")
full_list.remove("Alice")
full_list.remove("Bob")
full_list.remove("David")
print("These students did not submit: " + str(full_list))


