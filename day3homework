# Question 4 Task 1w

students = ["John", "Doe", "Jane", "Smith"]
grades = [85, 90, 78, 88]
activities = ["Football", "Music", "Art", "Dance"]

name = students.pop(2)
grade = grades.pop(2)
activity = activities.pop(2)

print(f"Student: {name}, failed {activity}, with a {grade}")

